{
  "name": "@ca0v/typescriptlab",
  "version": "1.0.0",
  "private": true,
  "description": "Nested package dependencies, l1, l2, l3, build into a single AMD module (l3/dist/index.js)",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "compile": "tsc",
    "watch:compile": "npm run compile -- -w",
    "watch:terser": "chokidar \"dist/index.js\" -d -t -c \"npm run terser\"",
    "terser": "terser node_modules/requirejs/require.js node_modules/jquery/dist/jquery.js src/yall.js dist/index.js -o dist/app.js --source-map \"url='./app.js.map',includeSources=true\"",
    "watch:server": "live-server --open=./src/app.html --watch=dist/app.js,src/app.html",
    "watch": "npm-run-all -p watch:compile watch:terser watch:server",
    "test": "@echo todo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ca0v/typescriptlab.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ca0v/typescriptlab/issues"
  },
  "homepage": "https://github.com/ca0v/typescriptlab#readme",
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "chokidar-cli": "^2.0.0",
    "lerna": "^3.16.4",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "terser": "^4.2.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@ca0v/l1": "file:l1",
    "@ca0v/l2": "file:l2",
    "@ca0v/l3": "file:l3",
    "requirejs": "^2.3.6"
  }
}
